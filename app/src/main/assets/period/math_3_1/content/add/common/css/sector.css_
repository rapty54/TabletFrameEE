/* half dashed line 가운데 점선있고, 양옆으로 나뉜 레이아웃 */
.halfDashedLine                                         {overflow: hidden;width: 100%; height: 576px;}
.halfDashedLine::after                                  {content: ""; display: block; clear: both;}
.halfDashedLine .halfItem                               {width: 49.2%; height: 100%; float: left; position: relative; text-align: center;}
.halfDashedLine .dashedLine                             {width: 1.4%;height: 100%;float: left;background: url("../img/dashed.png") no-repeat center;}
.halfDashedLine .dashedLine.whiteDot                    {background-image: url("../img/white_dot.png");}

