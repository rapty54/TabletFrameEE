.formHorizonQuiz 										{display:inline-block; line-height:90px;}
.formHorizonQuiz > span									{display: inline-block; line-height:90px; vertical-align: middle;}
.formHorizonQuiz .effectWrap							{margin-top:0px; line-height:75px; vertical-align: middle;}
.formHorizonQuiz .effectWrap .inner                     {height: 75px;}
.formHorizonQuiz .effectWrap .inner .txt                {margin-top: 5px;}

.formVertQuiz 											{display:inline-block; line-height:60px; margin:0; font-size:60px; letter-spacing: 44px; text-align: center; }
.formVertQuiz .line1									{position:relative; display:flex; justify-content:space-around; height:95px; padding-left:80px; }
.formVertQuiz .line1 span								{text-indent: 40px}
.formVertQuiz .line1 span:last-child					{letter-spacing: 40px;}
.formVertQuiz .line2									{position:relative; display:flex; justify-content:space-around; height:95px; padding-left:80px;}
.formVertQuiz .line2::before                            {content: "＋"; font-weight: 900; position: absolute; left: -10px;}
    /* 세로샘 기호 */
    .formVertQuiz.plus .line2::before                       {content: "＋";}
    .formVertQuiz.minus .line2::before                      {content: "－";}
    .formVertQuiz.multiple .line2::before                   {content: "×";}
    .formVertQuiz.divide .line2::before                     {content: "÷";}
.formVertQuiz .line2 span								{text-indent: 40px}
.formVertQuiz .line2 span:last-child					{letter-spacing: 40px;}
.formVertQuiz .line3									{position:relative; display:flex; justify-content:space-around; padding-left:40px; padding-top:20px; border-top:2px solid #000000; }
.formVertQuiz .effectWrap								{}
.formVertQuiz .effectWrap .inner						{position:relative; height:75px; /* display:flex;justify-content:space-around;*/ line-height: 75px; text-align: center; margin-right: -44px;}
.formVertQuiz .effectWrap .inner .txt                   {margin-top: 5px;}
.formVertQuiz .effectWrap .inner span					{text-indent: 40px}
.formVertQuiz .effectWrap .inner span:last-child		{letter-spacing: 40px;}
.formVertQuiz .effectWrap button                        {left: -22px !important;}



.formFraction 											{font-size:60px; line-height:160px; vertical-align: middle;}
.formFraction .effectWrap 								{vertical-align: middle;}
.formFraction .effectWrap .inner                        {overflow: visible;}
.formFraction .frac										{position:relative; display:inline-block; vertical-align: middle; height:110px; padding:0px 20px; margin-top: -20px;}
.formFraction .frac	.t									{position:absolute; top:0px; left:0; line-height:40px; padding:0px 5px; text-align: center;}
.formFraction .frac	.t:after  							{position:absolute; bottom:-22px; left:-8px; content:''; clear:both; display:block; width:calc(100% + 16px); height:0px; border-bottom:6px solid #000; }
.effectWrap .frac .t:after                              {border-color: #fe4100;}
.formFraction .frac	.b									{position:absolute; bottom:0px; left:0; line-height:40px; padding:0px 5px; text-align: center;}
.formFraction .integer /*분수 앞 정수*/                 {display: inline-block; margin-right: 0.15em; margin-left: -0.1em;}
.formFraction .effectWrap                               {padding: 25px 35px;}
.formFraction .effectWrap .frac                         {margin-top: 0;}




/* 팝업 - 레이어 전체창   ------------------------------------------------------------------------------------------------------------- */ 
.popFullWrap											{position:absolute; display:none; width: 100%; height:100%; top:0; left:0; z-index:200; background:rgba(0,0,0,0.7); border-radius:40px; /*animation:opacityMotion ease 0.1s; -webkit-animaition:opacityMotion ease 0.1s; -ms-animation:opacityMotion ease 0.1s;*/}
.popFullWrap.on											{display:block;}
.popFullWrap .inner										{position:absolute; top:50%; left:50%; width:100%; transform: translate(-50%, -50%); padding:60px 76px; background:#ffffff; border:6px solid #3f97d6; border-radius:30px;}
.popFullWrap .inner.full								{height:100%; top:0%; left:0%; width:100%; transform:none; padding:60px 76px; background:#ffffff; border:6px solid #3f97d6; border-radius:40px;}
.popFullWrap .btnClose 									{position:absolute; display:inline-block; top:20px; right:20px; width:54px; height:54px; background:url('../../common/img/btn/btnClosePop01.png') no-repeat;}

@keyframes opacityMotion								{ 0% {opacity:0;} 100% {opacity:1;} }
@-webkit-keyframes opacityMotion						{ 0% {opacity:0;} 100% {opacity:1;} }
@-ms-keyframes opacityMotion							{ 0% {opacity:0;} 100% {opacity:1;} }

.btnPopFullOpen 										{display:none;}
.btnPopFullOpen.show 									{display:block;}

/* 팝업 색 */
.popFullWrap.popupGreen .inner {border-color: #afd141;}
.popFullWrap.popupGreen .btnClose {background-image: url("../../common/img/btn/btnClosePop04.png");}
.popFullWrap.popupOrange .inner {border-color: #f89a33;}
.popFullWrap.popupOrange .btnClose {background-image: url("../../common/img/btn/btnClosePop05.png");}
.popFullWrap.popupPurple .inner {border-color: #b24bc0;}
.popFullWrap.popupPurple .btnClose {background-image: url("../../common/img/btn/btnClosePop06.png");}
.popFullWrap.popupTeal .inner {border-color: #1cb0c1;}
.popFullWrap.popupTeal .btnClose {background-image: url("../../common/img/btn/btnClosePop07.png");}

.popFullWrap.type2 .inner								{padding:40px 110px 40px 25px;}
.popFullWrap.type2 .effectWrap							{padding:0px; background:#ffffff; border:none; text-align: left; height:auto; margin:0; padding:0; border-radius:0;}
.popFullWrap.type2 .effectWrap span.exam 				{top:24px; left:14px;}
.popFullWrap.type2 .effectWrap span.exam + ul 			{padding-left:59px !important;}
/* 팝업 - 토스트 버튼    ----------------------------------------------------------------------------------------- -------------------- */ 
.popToastBtnWrap 										{position:absolute; top:0; left:0;}
.popToastBtnWrap .txt									{position:absolute; top:0; left:0; font-size:0; }
.popToastBtnWrap .btn									{position:absolute; top:50px; left:0; font-size:0;}

.btnPopToastOpen 										{display:none;}
.btnPopToastOpen.show 									{display:block;}
/*.btnPopToastOpen.on 									{cursor:default;}*/

.popToastWrap.topright .btnPopToastClose 				{position: absolute; top:-66px; right:6px; left: auto;}
.popToastWrap .btnPopToastClose 				        {position: absolute; top:-66px; right:auto; left: 117px;}

/* 팝업 - 토스트 팝업 영역    ------------------------------------------------------------------------------------------------------------- */ 
.popToastWrap											{position:absolute; display:none; top:calc(100% + 17px); left:calc(0%); z-index:200;  height:55px; /*animation:opacityMotion ease 0.5s; -webkit-animaition:opacityMotion ease 0.5s; -ms-animation:opacityMotion ease 0.5s;*/}
.popToastWrap.topright									{right:0; left:auto;}
.popToastWrap.on										{display:inline-block;}

.popToastWrap.oragne .box {border: 6px solid #fe880b; background-color: #fff; border-radius: 30px; font-size: 48px; font-weight: 500; padding: 20px 25px; width: 340px; text-align: left;}
.popToastWrap.oragne .txt {margin: 20px 0 10px 25px;}
.popGroup 												{z-index: 100;}

