@charset "utf-8";




/* 슬라이더 (내부 페이징) ------------------------------------------------------------------------------------------------------------- */ 
.sliderWrap 											{display: block; position: relative; height:100%;}
.sliderWrap .slideList  								{height:100%;}
.sliderWrap .slideList .slide 							{}
.sliderWrap .slideList .slide 							{display: none;  height:100%;}
.sliderWrap .slideList .slide.on 						{display: block !important;}
.sliderWrap .btnPrev 									{display: block; position: absolute; top:50%; left:15px; transform: translateY(-50%); font-size:0; background:url("../img/btn/btnPrevOn.png") no-repeat; width:27px; height:49px;}
.sliderWrap .btnNext 									{display: block; position: absolute; top:50%; right:15px; transform: translateY(-50%); font-size:0; background:url("../img/btn/btnNextOn.png") no-repeat; width:27px; height:49px;}
.sliderWrap .btnPrev.hide 								{display: none !important;}
.sliderWrap .btnNext.hide 								{display: none !important;}
.sliderWrap .btnPrev.dim 								{background:url("../img/btn/btnPrev.png") no-repeat; display:none;}
.sliderWrap .btnNext.dim 								{background:url("../img/btn/btnNext.png") no-repeat; display:none;}

/* 퀴즈 (전체화면, 인라인)  ------------------------------------------------------------------------------------------------------------- */ 
.groupWrap												{overflow: hidden;}
.effectWrap												{position:relative; display:inline-block; vertical-align: top; padding:15px 25px; background:#ffffff; border:4px solid #3e97d6; border-radius: 20px; overflow: hidden; text-align: left; color:#fe4100; overflow: hidden;}

    /* effectWrap 단계별 사이즈 */
.effectWrap.size0										{height: 246px;}
.effectWrap.size1										{height: 311px;}
.effectWrap.size1_1										{height: 336px;}
.effectWrap.size1_2										{height: 385px;}
.effectWrap.size2										{height: 391px;}
.effectWrap.size2_1										{height: 414px;}
.effectWrap.size3										{height: 464px;}

.effectWrap	.inner										{display:block; overflow: hidden;}
.effectWrap	.inner .txt									{line-height: 1;}
.effectWrap	.inner button								{position:absolute; top:0; left:0; width:100%; height:100%;  background:#fff url("../img/btn/btnQuizOff.png") no-repeat center center; background-size:72px 75px; z-index: 10; }
.effectWrap[data-free="true"] .inner button				{background:#fff url("../img/btn/btnQuiz.png") no-repeat center center; }
.effectWrap	.inner button.able							{background:#fff url("../img/btn/btnQuiz.png") no-repeat center center;}
.effectWrap	.inner button.trans							{background:none !important; cursor:default !important;}
.effectWrap	.inner button.btnOrderQ                     {background-image: url("../img/btn/btnQuizOff.png");}
.effectWrap	.inner button.btnOrderQ.btnQuizbg           {background-image: url("../img/btn/btnQuiz.png");}

/* 퀴즈 안에 빨간 동그라미 예 */
.effectWrap.type1                                       {padding:15px 25px !important;}
.effectWrap.type1 .exam                                 {display:inline-block; width:50px; height:50px; line-height:48px; font-size:28px; vertical-align:top; text-indent:10px; border-radius:50%; background-color:#fe4100; color:#fff;letter-spacing: 0; position:absolute; top:26px; left:46px;}
.effectWrap.type1 ul.dots                               {padding-left: 80px;}
.effectWrap.type1 ul.dots li:before                     {content:'·'; position:absolute; top:0; left:10px;}
.effectWrap.type1 ul.dots li                            {padding-left:50px; position:relative;font-size:50px; font-weight:500; line-height:66px !important;}
    /* 불릿없고 텍스트만  */
.effectWrap.type1 .exam_txt                             {padding-left:100px; display: inline;font-size:50px; font-weight:500; line-height:66px !important;}

.effectWrap.type3										{padding-left:50px; padding-right:50px;}
.effectWrap.type3 .exam                                 {position:absolute; top:26px; left:50px; display:inline-block; width:50px; height:50px; line-height:48px; font-size:28px; vertical-align:top; text-indent:10px; border-radius:50%; background-color:#fe4100; color:#fff;letter-spacing: 0;}
.effectWrap.type3 .exam_txt                             {padding-left:70px; display: inline;font-size:50px; font-weight:500; line-height:66px !important;}

.effectWrap.type4										{padding-left:50px; padding-right:50px;}
.effectWrap.type4 .exam                                 {position:absolute; top:26px; left:50px; display:inline-block; width:50px; height:50px; line-height:48px; font-size:28px; vertical-align:top; text-indent:10px; border-radius:50%; background-color:#fe4100; color:#fff;letter-spacing: 0;}
.effectWrap.type4 .exam_txt                             {padding-left:70px; display: inline;font-size:50px; font-weight:500; line-height:66px !important;}

.effectOuter 											{text-align: left; line-height:98px; }
.effectOuter2 											{text-align: left; line-height:98px; background:#e1effa; border-radius: 20px;}


/* 연산기호만 굵기가 더 두꺼워서 span으로 감싸기 */
.weight700 {font-weight: 700;}
.weight900 {font-weight: 900;}

